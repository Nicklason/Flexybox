@using System.Diagnostics;

@page "/"

@code {
  private RestaurantModel? restaurant;

  protected override void OnInitialized()
  {
    restaurant = RestaurantService.GetByName("Aalborg");
    Debug.Assert(restaurant != null, "Restaurant should not be null");
  }
}

<PageTitle>Home</PageTitle>

<Restaurant Name="@restaurant.Name" Images="@restaurant.Images" Address="@restaurant.Address" Contact="@restaurant.Contact" OpenHours="@restaurant.OpenHours" />
